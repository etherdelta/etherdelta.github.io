<a href="javascript:;" class="dropdown-toggle" data-toggle="dropdown" role="button" aria-haspopup="true" aria-expanded="false"><i class="fa fa-file-text"></i> <span class="trn">Smart_Contract</span> <span class="caret"></span></a>
<ul class="dropdown-menu">
  <li><a href="javascript:;"><span class="trn">connection</span>: <%= connection.connection %> (<%= connection.provider %>) <%= connection.testnet ? 'TESTNET' : ''%></a></li>
  <li><a href="<%= contractLink%>" target="_blank"><span class="trn">etherscan_contract</span></a></li>
