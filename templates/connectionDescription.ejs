  <li role="separator" class="divider"></li>
  <% contracts.forEach(function(contract){ %>
    <li style="<%= contractAddr==contract.addr ? 'background: #999;' : ''%>"><a href="javascript:;" onclick="bundle.EtherDelta.contractAddr('<%= contract.addr %>');"><%= contract.addr.substring(0,12) %>... (<%= contract.info %>)</a></li>
  <%}) %>
</ul>
